{
  "name": "nurviov5_admin",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm-run-all -p dev:backend dev:frontend",
    "dev:backend": "npm --workspace apps/backend run dev",
    "dev:frontend": "npm --workspace apps/frontend run dev",
    "build": "npm-run-all build:backend build:frontend",
    "build:backend": "npm --workspace apps/backend run build",
    "build:frontend": "npm --workspace apps/frontend run build",
    "postinstall": "npm --workspace apps/backend run prisma:generate || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "npm-run-all": "^4.1.5"
  }
}
